<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefWidth="450.0" styleClass="body" stylesheets="@weather.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="timetable.weather.WeatherController">

    <Label fx:id="temp" layoutX="122.0" layoutY="112.0" text="temp" />
    <ImageView fx:id="weatherIcon" fitHeight="92.0" fitWidth="92.0" layoutX="15.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">

        <Image url="@../resources/images/weather/unknown.png" />

    </ImageView>
    <ImageView fx:id="compass" fitHeight="55.0" fitWidth="58.0" layoutX="330.0" layoutY="96.0" pickOnBounds="true" preserveRatio="true">

        <Image url="@../resources/images/compass.png" />

    </ImageView>
    <ImageView fx:id="arrow" fitHeight="27.0" fitWidth="28.0" layoutX="344.0" layoutY="110.0" pickOnBounds="true" preserveRatio="true" rotate="45.0">

        <Image url="@../resources/images/arrow.png" />

    </ImageView>
    <Button contentDisplay="CENTER" ellipsisString="" layoutX="193.0" layoutY="181.0" mnemonicParsing="false" onAction="#close" text="Close" textOverrun="CLIP" />
    <Label fx:id="condition" layoutX="121.0" layoutY="50.0" text="Condition">
        <font>
            <Font size="24.0" />
        </font>
    </Label>
    <Label fx:id="windSpeed" layoutX="121.0" layoutY="151.0" text="???Kph" />
    <Label fx:id="humid" layoutX="122.0" layoutY="131.0" text="humid" />
   <Label fx:id="city" layoutX="121.0" layoutY="94.0" text="City" />

    <padding>
        <Insets bottom="10.0" />
    </padding>
</AnchorPane>
