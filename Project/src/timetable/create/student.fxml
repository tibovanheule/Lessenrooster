<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="rootPane2" prefHeight="450.0" prefWidth="450.0" styleClass="body"
            stylesheets="@create.css" xmlns="http://javafx.com/javafx/9">
    <children>
        <AnchorPane prefHeight="100.0" prefWidth="450.0" styleClass="blue">
            <children>
                <Label layoutX="36.0" layoutY="42.0" styleClass="blue" text="Create Student"/>
            </children>
        </AnchorPane>
        <Button layoutX="84.0" layoutY="260.0" mnemonicParsing="false" onAction="#saveStudent"
                text="Save &amp; dont close"/>
        <Button layoutX="84.0" layoutY="388.0" mnemonicParsing="false" onAction="#close" text="Close"/>
        <Button layoutX="368.0" layoutY="120.0" mnemonicParsing="false" onAction="#menu" text="Menu"/>
        <TextField fx:id="name" layoutX="84.0" layoutY="225.0" promptText="Student group"/>
    </children>
</AnchorPane>
