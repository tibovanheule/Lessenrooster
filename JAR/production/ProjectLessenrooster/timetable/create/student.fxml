<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootPane2" prefHeight="450.0" prefWidth="450.0" styleClass="body" stylesheets="@create.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane prefHeight="100.0" prefWidth="450.0" styleClass="blue">
