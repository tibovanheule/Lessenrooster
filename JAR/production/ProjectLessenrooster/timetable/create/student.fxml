                <Label layoutX="36.0" layoutY="42.0" styleClass="blue" text="Create Student" />
            </children>
        </AnchorPane>
        <Button layoutX="153.0" layoutY="388.0" mnemonicParsing="false" onAction="#saveStudent" text="Save &amp; dont close" />
        <Button layoutX="84.0" layoutY="388.0" mnemonicParsing="false" onAction="#close" text="Close" />
        <Button layoutX="368.0" layoutY="120.0" mnemonicParsing="false" onAction="#menu" text="Menu" />
      <TableView fx:id="table" layoutX="84.0" layoutY="147.0" prefHeight="200.0" prefWidth="269.0">
        <columns>
          <TableColumn fx:id="name" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="75.0" text="student" />
          <TableColumn fx:id="delete" maxWidth="-1.0" prefWidth="-1.0" text="delete" />
        </columns>
