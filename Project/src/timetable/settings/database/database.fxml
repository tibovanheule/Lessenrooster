<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="timetable.settings.database.DatabaseController">
   <children>
      <AnchorPane prefHeight="130.0" prefWidth="607.0" styleClass="blue" stylesheets="@database.css">
         <children>
            <Label layoutX="54.0" layoutY="57.0" styleClass="blue" text="DATABASE SETTINGS" />
         </children>
      </AnchorPane>
      <CheckBox fx:id="mysql" layoutX="54.0" layoutY="151.0" mnemonicParsing="false" onAction="#mysql" text="Use Mysql?" />
      <ImageView fitHeight="177.0" fitWidth="384.0" layoutX="36.0" layoutY="200.0" onDragOver="#dragOver" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../resources/images/drag.png" />
         </image>
      </ImageView>
      <Label layoutX="53.0" layoutY="183.0" text="Drag &amp; Drop your file or click it to select the sqlite DB" />
      <Button layoutX="69.0" layoutY="365.0" mnemonicParsing="false" onAction="#close" text="Button" />
   </children>
</AnchorPane>
